.healthbar-root {
    width: 100px;
    height: 10px;
    background-color: #444;
    border-radius: 5px;
    justify-content: flex-start;
    align-items: center;
    padding: 1px;
    position: relative;
    left: -42px;
}

.healthbar-fill {
    width: 100%;
    height: 100%;
    background-color: red;
    border-radius: 5px;
}
